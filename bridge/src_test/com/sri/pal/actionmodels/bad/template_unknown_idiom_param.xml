<!-- $Id: template_unknown_idiom_param.xml 7401 2016-03-25 20:18:20Z Chris Jones (E24486) $ -->
<actionModel version="1.0">
    <!-- Idiom template action param references a parameter of this
         idiom which isn't defined. -->
    <family id="fam1">
        <inputParam role="role1"/>
    </family>

    <idiom id="move">
        <description/>
        <inputParam id="input1"/>

        <template>
            <action family="fam1">
                <idiomParam id="input2" role="role1"/>
            </action>
        </template>
    </idiom>
</actionModel>
