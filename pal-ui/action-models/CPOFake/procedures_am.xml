<actionModel version="0.1">

    <require url="./core_types.xml"/>

    <!-- Declared learned procedures -->
    <action id="Get all SIGACTs within a specified date range">
        <description>
            Get a list of all the SIGACTs within the given range of dates
        </description>
        <metadata key="name" value="Get all SIGACTs within a specified date range"/>
        <metadata key="icon" value="./gear.png"/>
        <effectsType type="has_side_effects"/>
        <inputParam id="Date range">
            <description>
                The range of dates to filter for
            </description>
            <typeRef typeId="date range"/>
        </inputParam>
        <outputParam id="SIGACTs">
            <typeRef typeId="event list"/>
        </outputParam>
    </action>

</actionModel>
